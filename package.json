{
  "name": "modernstack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "uv sync --project api && uv run --project api fastapi dev api/index.py",
    "next-dev": "next dev",
    "auth-generate": "pnpx @better-auth/cli@1.3.4 generate --yes --output ./utils/auth-schema.ts",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "ultracite fix --unsafe",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp"
    ]
  },
  "dependencies": {
    "@dicebear/collection": "^9.2.4",
    "@dicebear/core": "^9.2.4",
    "@floating-ui/react": "^0.27.16",
    "@libsql/client": "^0.15.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-form": "^1.19.3",
    "better-auth": "^1.3.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "radix-ui": "^1.4.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.4",
    "motion": "^12.23.12",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "ultracite": "5.3.7",
    "valibot": "^1.1.0"
  }
}
