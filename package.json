{
  "name": "modernstack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "uv sync --project api && uv run --project api fastapi dev api/index.py",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "ultracite fix --unsafe"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp"
    ]
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "ultracite": "5.3.7"
  }
